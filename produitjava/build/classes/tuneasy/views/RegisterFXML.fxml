<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="400.0" prefWidth="309.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tuneasy.views.RegisterFXMLController">
   <children>
      <TextField fx:id="firstNameTextField" layoutX="80.0" layoutY="56.0" promptText="first name ..." />
      <TextField fx:id="lastNameTextField" layoutX="80.0" layoutY="92.0" promptText="Last Name..." />
      <TextField fx:id="emailTextField" layoutX="80.0" layoutY="128.0" promptText="E-mail..." />
      <TextField fx:id="imageTextField" layoutX="81.0" layoutY="240.0" promptText="Image URL..." />
      <Button fx:id="registerButton" layoutX="116.0" layoutY="320.0" mnemonicParsing="false" text="Register" />
      <PasswordField fx:id="passwordTextField" layoutX="80.0" layoutY="165.0" promptText="Password" />
      <PasswordField fx:id="rePasswordTextField" layoutX="81.0" layoutY="203.0" promptText="Re-type Password" />
      <Button fx:id="loginButton" layoutX="71.0" layoutY="361.0" mnemonicParsing="false" text="Already have an account?" />
      <Label layoutX="14.0" layoutY="14.0" text="Signup">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="roleComboBox" layoutX="80.0" layoutY="277.0" prefWidth="150.0" promptText="Role" />
   </children>
</AnchorPane>
